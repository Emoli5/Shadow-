					local req = require(tool.GunStates)
					req.FireRate = tonumber(args[1])
					log('[ ✅ ]: Set fire rate for '..tool.Name..' to '..args[1])
				else log('[ ❌ ]: Input a valid number') end
			else log('[ ❌ ]: Equip a gun before using this command') end
		else log('[ ❌ ]: Equip a gun before using this command') end
