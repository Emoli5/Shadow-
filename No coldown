local badsf = require(game:GetService("ReplicatedStorage")["Shared_Modules"].Tools["Wave 4"]["Evil Starfire"]["Shared_StarVolley"])
local tri1 = require(game:GetService("ReplicatedStorage")["Shared_Modules"].Tools["Wave 4"].Trigon["Shared_GroundShatter"])
