local Tab = Window:NewTab("Hand Auto Buys")

local Section = Tab:NewSection("auto buys Hand")

Section:NewToggle("auto buy Dart", "auto buys", function(state)
    if state then
        _G.on = true
        while _G.on do
            wait()
            local args = {
                [1] = "Hand Yellow"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
        end
    else
        _G.on = false
    end
end)
